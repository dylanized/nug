{
  "ext": ".html",
  "scripts": ["https://code.jquery.com/jquery-3.7.1.slim.min.js", "/js/app.js"],
  "stylesheets": [
    "/css/reset.css",
    "/css/base.css",
    "/css/variants.css",
    "/css/util.css",
    "/css/media-md.css",
    "/css/media-lg.css"
  ],
  "theme": {
    "blockquoteBorderColor": "5px",
    "blockquoteBorderWidth": "manilla",
    "bodyBgColor": "white",
    "bodyListType": "square",
    "bodyTextColor": "charcoal",
    "bodyFont": "serif",
    "borderColor": "cloud",
    "breakpointTablet": "768px",
    "breakpointDesktop": "1100px",
    "containerWidth": "900px",
    "primaryColor": "grapefruit",
    "secondaryColor": "charcoal",
    "tertiaryColor": "whale",
    "headingFont": "verdana",
    "tableBorderColor": "whale",
    "tableBorderWidth": "4px",
    "tableTdBgColor": "white",
    "tableTdBorderColor": "cloud",
    "tableTdBorderWidth": "1px",
    "tableTdTextColor": "white",
    "tableThBgColor": "cloud",
    "tableThBorderColor": "charcoal",
    "tableThBorderWidth": "1px",
    "tableThTextColor": "white"
  },
  "variants": {
    "success": {
      "highlight": "canvas",
      "hover": "white",
      "link": "canvas",
      "matte": "lima"
    },
    "danger": {
      "highlight": "cardinal",
      "link": "maple",
      "matte": "rosey"
    },
    "warning": {
      "highlight": "sunshine",
      "hover": "mustard",
      "link": "charcoal",
      "matte": "manilla"
    },
    "info": {
      "highlight": "cobalt",
      "link": "ocean",
      "matte": "skyline"
    },
    "success-alt": {
      "highlight": "lima",
      "link": "white",
      "matte": "canvas",
      "text": "white"
    },
    "danger-alt": {
      "highlight": "rosey",
      "link": "white",
      "matte": "cardinal",
      "text": "white"
    },
    "warning-alt": {
      "highlight": "manilla",
      "hover": "mustard",
      "link": "charcoal",
      "matte": "sunshine",
      "text": "white"
    },
    "info-alt": {
      "highlight": "skyline",
      "link": "white",
      "matte": "cobalt",
      "text": "white"
    },
    "contrast": {
      "highlight": "charcoal",
      "hover": "royal",
      "matte": "white"
    },
    "grayscale": {
      "highlight": "aluminum",
      "hover": "royal",
      "matte": "cloud"
    },
    "midnite": {
      "highlight": "slate",
      "hover": "aluminum",
      "link": "white",
      "matte": "charcoal",
      "text": "white"
    }
